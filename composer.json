{
  "name": "smart/browsersession",
  "type": "library",
  "description": "Portable and cacheable browser sessions",
  "keywords": [
    "Browser",
    "Session"
  ],
  "homepage": "https://github.com/smart/browsersession",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Bull",
      "email": "me@gabrielbull.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "doctrine/orm": "~2",
    "jms/serializer": "~0.16",
    "sinergi/token": "~0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4",
    "klein/klein": "~2",
    "gabrielbull/router": "dev-develop",
    "predis/predis": "~1"
  },
  "autoload": {
    "psr-4": {
      "Smart\\BrowserSession\\": "src/BrowserSession"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smart\\BrowserSession\\Tests\\": [
        "tests/BrowserSession/Tests",
        "tests/BrowserSession/Tests/_includes"
      ]
    }
  }
}
